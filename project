#include <iostream>

using namespace std;

int main() {
    int sumi[10] = {54, 67, 765, 54, 43, 76, 875, 43543, 6546, 654};
    printf("int: \n");
    for (int i: sumi) {
        printf("%d\n", i);
    }

    printf("\n");

    short sums[10] = {54, 67, 765, 54, 43, 76, 875, 0, 6546, 654};
    printf("short: \n");
    for (short i: sums) {
        printf("%hd\n", i);
    }

    printf("\n");

    char sumc[10] = {54, 67, 2, 54, 43, 76, 4, 12, 42, 'f'};
    printf("char: \n");
    for (char i: sumc) {
        printf("%c\n", i);
    }

    printf("\n");

    long suml[10] = {54, 67, 765, 54, 43, 76, 875, 43543, 6546, 654};
    printf("long: \n");
    for (long i: suml) {
        printf("%ld\n", i);
    }

    printf("\n");

    bool sumb[10] = {true, true, true, true};
    printf("bool: \n");
    for (bool i: sumb) {
        cout << i << "\n";
    }

    printf("\n");

    float sumf[10] = {54.1, 67, 765, 54, 43, 76, 875, 43543, 6546.5, 654};
    printf("float: \n");
    for (float i: sumf) {
        printf("%f\n", i);
    }

    printf("\n");

    double sumd[10] = {54, 67.9, 765, 54, 43, 76, 875, 43543, 6546.001, 654};
    printf("double: \n");
    for (double i: sumd) {
        printf("%f\n", i);
    }

    printf("\n");

    string sumstr[10] = {"54", "67", "765", "54", "43", "76", "875", "43543", "6546", " 654"};
    printf("string: \n");
    for (const string &i: sumstr) {
        printf("%s\n", i.c_str());
    }

    printf("\n");
}
